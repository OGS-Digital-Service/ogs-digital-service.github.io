<!doctype html>
<html lang="en">
  <head>
    
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="./css/tailwind-dev.css" rel="stylesheet">
    <link href="./css/fonts.css" rel="stylesheet">
    {% include '_css-global.njk' %}
    <title>{{ title }}</title>
  </head>

  <body class="mx-8 md:mx-20 flex flex-col flex-row">
    {% from '../_includes/_components.njk' import unav,nav %}

    {{ unav({ 
    intro: "An Official Website of New York State",
    nyslogo: "nygov-logo.png",
    arialabel: "New York State Logo",
    alert:"",
    warning:""
      })}}

    {{ nav({ 
    agencyname: "Digital Services",
    link: "/",
    arialabel: "New York State Digital Services"
      })}}
    
    {{ content | safe }}
  </body>
</html>
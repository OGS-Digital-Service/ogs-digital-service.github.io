<!doctype html>
<html lang="en">
  <!-- If you're reading this: help us obi-wan can-coder, you're our only hope. NYS is committed to Digital Accessibility. Apply today: digital.services@ogs.ny.gov -->
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="{{title}}">
    <meta name="generator" content="11ty/eleventy">   
    <meta property="og:url" content="https://{{url}}">
    <meta property="og:type" content="website">
    <meta property="og:title" content="{{title}}">
    <meta property="og:description" content="{{description}}"> 
    {% include '_css-link.njk' %}
    <title>{{ title }}</title>
  </head>
  <body class="">
    {% from '../_includes/_components.njk' import backtop, unav  %}
  <a href="#main-site-content-for-skiplink" class="sr-only focus:not-sr-only underline">Skip to Main Content</a>
    <main class="nysds-a11y-main" id="main-site-content-for-skiplink">
      {{ 
        unav({})
      }} 
      <section class="nysds-a11y-document">
      {{ content | safe }}
      </section>
      {{
        backtop({
            link:"#table-contents",
            text:"Top",
            label: "to table of contents" 
        })
      }}
    </main>

  </body>
</html>